# ============================================================================
# Makefile for The AI Observatory - Beginner Level
# ============================================================================
# This Makefile provides convenient commands for running
# the HubSystem CLI application.
# ============================================================================

.PHONY: help hubsystem

# Default target - show help
help:
	@echo "The AI Observatory - Available Commands:"
	@echo ""
	@echo "Application:"
	@echo "  make hubsystem   - Start the HubSystem CLI"

# ----------------------------------------------------------------------------
# Application Targets
# ----------------------------------------------------------------------------

# Start the HubSystem CLI
hubsystem:
	@python hubsystem.py
