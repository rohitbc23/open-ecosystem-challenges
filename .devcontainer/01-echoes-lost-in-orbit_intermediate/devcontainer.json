{
  "name": "Adventure 01 | \uD83D\uDFE1 Intermediate (The Silent Canary)",
  "image": "mcr.microsoft.com/devcontainers/base:bullseye",
  "features": {
    "ghcr.io/devcontainers/features/docker-in-docker:2": {
    }
  },
  "postCreateCommand": "bash .devcontainer/01-echoes-lost-in-orbit_intermediate/post-create.sh",
  "postStartCommand": "bash .devcontainer/01-echoes-lost-in-orbit_intermediate/post-start.sh",
  "customizations": {
    "codespaces": {
      "openFiles": [
        "adventures/01-echoes-lost-in-orbit/README.md"
      ]
    }
  },
  "forwardPorts": [
    30100,
    30101,
    30102
  ],
  "portsAttributes": {
    "30100": {
      "label": "ArgoCD",
      "onAutoForward": "notify"
    },
    "30101": {
      "label": "Argo Rollouts Dashboard",
      "onAutoForward": "notify"
    },
    "30102": {
      "label": "Prometheus",
      "onAutoForward": "notify"
    }
  },
  "otherPortsAttributes": {
    "onAutoForward": "ignore"
  }
}